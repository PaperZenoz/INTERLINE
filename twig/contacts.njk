{% set page = { title: 'Контакты' } %}
{% extends "./2.parts/main.njk" %}
{% block content %}
    {% set  _ = 'breadcrumbs' %}
    <div class="{{ _ }}">
        <div class="container-lg">
            <div class="{{ _ }}__wrap">
                {% set  list = [
                    'Главная',
                    'Контакты'] %}

                {% for item in list %}
                    <a href="#" class="{{ _ }}__link">
                        {{ item }}
                    </a>

                {% endfor %}
            </div>
        </div>
    </div>

    {% set  _ = 'contacts' %}
